#import cld2
encrypted_input = raw_input("Enter the encrypted text :")
shift = 1
def decrypt(shift):
    decrypted_text = ""
    for i, encrypted_char in enumerate (encrypted_input):
        ascii_value = int(ord(encrypted_char)) + shift                   #ascii range 33 to 126
        if ascii_value > 126:
            ascii_value = int(ascii_value) - 94
        decrypted_char = chr(ascii_value)
        decrypted_text = decrypted_text + decrypted_char
    return (decrypted_text)

def plaintextcheck(decrypted_text):
    decrypted_text.encode('utf-8')
    isreliable, textbytesfound, details = cld.detect(decrypted_text)
    if details([0][0]) == 'english':
        print decrypted_text.decode('utf-8')


while (shift < 93):
    #plaintextcheck(decrypt(shift))
    print(decrypt(shift))
    shift += 1

